<div class="container bs-docs-container"> 
	<div class="row"> 
		<div class="col-md-9" role="main">
			[% INCLUDE alertbar.tt %]
			<section id="rejestracje">
				<div class="panel panel-default">
					<div class="panel-heading">
						Rejestracja nowego u≈ºytkownika
					</div>
					<div class="panel-body">
						<div class="row"> 
							<div class="col-md-8">
								<form action="register" method="POST" role="form">
									<div class="form-group" id="registration-form">
										[% FOREACH field IN form %]
										<div class="input-group">
											<span class="input-group-addon">[% field.text %]</span>
											<input 
											name="[% field.name %]"
											[% IF field.type != 'radio' %]
											value="[% submitted.${field.name} %]"
											[% END %]
											type="[% field.type %]"
											class="form-control"
											placeholder="[% field.text %]"
											aria-describedby="basic-addon1"
											[% FOREACH add IN field.additional %]
											[% add %]
											[% END %]
											>
										</div>
										[% END %]
										<div class="text-center">
											<button type="submit" class="btn btn-default">Zarejestruj mnie</button>
										</div>
									</form>
								</div>
							</div>
							<div class="col-md-4">
								<div class="panel panel-default">
									INFO
								</div>
							</div>
						</div>
					</div>
				</div>
			</section>

		</div>
		[% INCLUDE sidebar.tt %]
	</div>
</div> <!-- /container -->
<script src="[% request.uri_base %]/javascripts/validations.js"></script>
