<!DOCTYPE html>
<section id="registration_info">
    <div class="panel panel-primary">
        <div class="panel-heading">
            <h3 class="panel-title">Opis kierunku</h3>
        </div>
        <div class="panel-body">

        [% FOREACH info1 IN info %]
            <table width=100% align="center">
                [% FOREACH field IN info1.main %]
                <tr>
                    <td width=25%>[% field.label %]</td>
                    <td>[% field.item %]</td>
                </tr>
                [% END %]
            </table>
            <br>

            <!-- Tab -->
            <div class="tab">
            [% FOREACH field IN info1.tabs %]
                <div class="tab-painel">
                    <input class="tab-open" id="[% field.item %]" name="tab-wrap-1" type="radio" checked>
                    <label class="tab-nav" for="[% field.item %]">[% field.label %]</label>
                    <div class="tab-inner">
                        [% field.item %] [% field.item %] [% field.item %]
                    </div>
                </div>
            [% END %]
        [% END %]

        </div>
    </div>
</section>
