<!DOCTYPE html>
<section id="rejestracje">
	<div class="panel panel-primary">
		<div class="panel-heading">
			<h3 class="panel-title">Aktywne rejestracje</h3>
		</div>
		[% IF registrations %]
		<div class="panel-body">
			[% FOREACH registration in registrations %]
			
			<div class="panel panel-info">
				<div class="panel-heading">
					<h3 class="panel-title" style="display:inline-block">
                        <a href="">[% registration.name %]</a></h3>
                    <a href=""><img src="/images/button-buy.png" align="right"
                            alt="Rejestracja" style="width:30px;height:30px;"></a>
                    <a href=""><img src="/images/button-info.png" align="right"
                            alt="Informacja" style="width:30px;height:30px;margin-right:10px;"></a>
				</div>
				<div class="panel-body">
					Data: [% registration.end_date %]<br>
					Limit miejsc: [% registration.limit %]
				</div>
			</div>

			[% END %]
		</div>
		[% ELSE %]
		<div class="panel-body">
			Obecnie nie ma Å¼adnych aktywnych rejestracji.
		</div>
		[% END %]					
	</div>
</section>
