<div class="container bs-docs-container"> 
	<div class="row">
		<table class="table">
			<tbody>
				<td width=80%>
					[% INCLUDE alertbar.tt %]
					<section id="rejestracje">
						<div class="panel panel-primary">
							<div class="panel-heading">
								<h3 class="panel-title">Aktywne rejestracje</h3>
							</div>
							[% IF registrations %]
							<div class="panel-body">
								[% FOREACH registration in registrations %]
								
								<div class="panel panel-info">
									<div class="panel-heading">
										<h3 class="panel-title">Przedmiot: [% registration.name %]</h3>
									</div>
									<div class="panel-body">
										Data: [% registration.due %]<br>
										Zarejestrowano: [% registration.registered %]/[% registration.limit %]
									</div>
								</div>

								[% END %]
							</div>
							[% ELSE %]
							<div class="panel-body">
								Obecnie nie ma Å¼adnych aktywnych rejestracji.
							</div>
							[% END %]					
						</div>
					</section>
				</td>
				<td width=20%>
					[% INCLUDE sidebar.tt %]
				</td>
			</tbody>
		</table>
	</div>
</div> <!-- /container -->


